<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

    <PropertyGroup>
        <CloudTargetProfile>Cloud</CloudTargetProfile>
    </PropertyGroup>
  
    <ItemGroup>
        <Tokens Include="Azure_subID">
            <ReplacementValue>xxx</ReplacementValue>
        </Tokens>
        <Tokens Include="Azure_subName">
            <ReplacementValue>xxx</ReplacementValue>
        </Tokens>
        <Tokens Include="Azure_thumbprint">
            <ReplacementValue>xxx</ReplacementValue>
        </Tokens>
        <Tokens Include="Azure_profile">
            <ReplacementValue>$(CloudTargetProfile)</ReplacementValue>
        </Tokens>
        <Tokens Include="Azure_waitRunning">
            <ReplacementValue>true</ReplacementValue>
        </Tokens>
    </ItemGroup>
</Project>