<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <system.web>
    <compilation debug="false" xdt:Transform="SetAttributes" />
    <customErrors mode="On" xdt:Transform="SetAttributes" />
  </system.web>
  <system.identityModel>
    <identityConfiguration>
      <securityTokenHandlers>
        <securityTokenHandlerConfiguration>
          <audienceUris>
            <add value="http://bermdev.cloudapp.net/" xdt:Transform="Insert" />
          </audienceUris>
        </securityTokenHandlerConfiguration>
      </securityTokenHandlers>
    </identityConfiguration>
  </system.identityModel>
  <system.identityModel.services>
    <federationConfiguration>
      <wsFederation
        issuer="https://bermacs.accesscontrol.windows.net/v2/wsfederation"
        realm="http://bermdev.cloudapp.net/" xdt:Transform="SetAttributes" />
    </federationConfiguration>
  </system.identityModel.services>
  <appSettings>
    <add key="RouteDebugger:Enabled" value="false" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
  </appSettings>
  <log4net>
    <root>
      <level value="INFO" xdt:Transform="SetAttributes" />
      <appender-ref xdt:Transform="RemoveAll"/>
      <appender-ref ref="TraceAppender" xdt:Transform="Insert"/>
    </root>
    <logger xdt:Transform="RemoveAll" />
    <logger name="NHibernate" additivity="true" xdt:Transform="Insert">
      <level value="ERROR" />
    </logger>
  </log4net>
</configuration>